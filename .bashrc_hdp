# .bashrc

# User specific aliases and functions

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi


export AMBARI_SERVER_HOSTNAME=$HOSTNAME
export AMBARI_ADMIN_PASSWORD=admin
export CODE_REPO=http://github.com/feeroz123/hdp_roc
export CLUSTER_NAME=single-node-hdp-cluster
export HDP_STACK_VERSION=2.6
export BLUEPRINT_FILE=cluster_configuration.json
export JAVA_HOME=/usr/jdk64/jdk1.8.0_112
export PATH=$PATH:$JAVA_HOME/bin
export MYSQL_ROOT_PASSWD=root123
